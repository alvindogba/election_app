<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Chairman Dashboard</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css" integrity="sha512-Kc323vGBEqzTmouAECnVceyQqyqdsSiqLQISBL29aUW4U/M7pSPA/gEUZQqv1cwx4OnYxTxve5UMg5GT6L4JJg==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="css/dashboard.css">
  </head>
  <body>
    <header class="navbar bg-body-tertiary">
        <nav class="container nav justify-content-between" >
            <div class=""><a class="navbar-brand" href="#">Logo</a></div>
            <div class="">icon1 &nbsp; icon2</div>
        </nav>
    </header>
    <!-- SideBar -->
     <main id="main">
            <ul class="nav flex-column bg-body-tertiary" id="sidebar">
        <li class="nav-item">
            <button class="dashboard_title side_btn"><h4>Dashboard</h4></button>
        </li>
        <li class="nav-item">
            <button class="voters side_btn"><h5>Voters</h5></button>
        </li>
        <li class="nav-item">
            <button class="Parties side_btn"><h5>Parties</h5></button>
        </li>
        <li class="nav-item">
          <button class="contestants side_btn"><h5>contestants</h5></button>
        </li>
        <li class="nav-item">
            <button class="contestants side_btn"><h5>contestants</h5></button>
        </li>
      </ul>
      
      <section class="result_dashboard">
        <h1>Result Dashboard</h1>
        <div class="container" id="reg_voter_total_voters_flex_container">
            <div class="cards  bg-body-tertiary">
                <i class="fa-solid fa-pencil"></i>
                <div class="card-body">
                  <h5 class="card-title">Registered voters</h5>
                  <p class="card-text">Total numbers of registered voters</p>
                </div>
                <span>10</span>
              </div>

              <!-- Card 2 -->
              <div class="cards  bg-body-tertiary">
                <i class="fa-solid fa-scale-unbalanced-flip"></i>
                <div class="card-body">
                  <h5 class="card-title">Total Votes</h5>
                  <p class="card-text">Total numbers of  votes</p>
                </div>
                <span>10</span>
              </div>
        </div>

        <div id="section2">
            <h1>Chairman</h1>

            <div class="container d-flex justify-content-between">
                <div class="card" style="width: 13rem;">
                    <!-- <i class="fa-solid fa-circle-user"></i> -->
                     <img src="<%=data[0].president_image %>" alt="">
                    <div class="card-body">
                      <h5 class="card-title"><%= data[0].president_name%></h5>
                      <h4 style="color: blue"><%=data[0].abbreviation %></h4>
                      <div class="progress" role="progressbar" aria-label="Success example" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
                        <div class="progress-bar bg-info" style="width: 25%">25%</div>
                      </div>
                    </div>
                  </div>
                  <!-- Card 2 -->
                  <div class="card" style="width: 13rem;">
                    <i class="fa-solid fa-circle-user"></i>
                    <div class="card-body">
                      <h5 class="card-title">John Brown</h5>
                      <div class="progress" role="progressbar" aria-label="Success example" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
                        <div class="progress-bar bg-warning" style="width: 25%">25%</div>
                      </div>
                    </div>
                  </div>
                  <!-- card3 -->
                  <div class="card" style="width: 13rem;">
                    <i class="fa-solid fa-circle-user"></i>
                    <div class="card-body">
                      <h5 class="card-title">John Brown</h5>

                      <div class="progress" role="progressbar" aria-label="Success example" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
                        <div class="progress-bar bg-success" style="width: 25%">25%</div>
                      </div>
                    </div>
                  </div>
                  <!-- card 4 -->
                  <div class="card" style="width: 13rem;">
                    <!-- <img src="..." class="card-img-top" alt="..."> -->
                    <i class="fa-solid fa-circle-user"></i>
                    <div class="card-body">
                      <h5 class="card-title">John Brown</h5>

                      <div class="progress" role="progressbar" aria-label="Success example" aria-valuenow="25" aria-valuemin="0" aria-valuemax="100">
                        <div class="progress-bar bg-danger" style="width: 25%">25%</div>
                      </div>
            </div>
          </div>
      </section>
     </main>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    <script src="js/dashboard.js"></script>
  </body>
</html>
